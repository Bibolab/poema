        "reflect-metadata": "^0.1.3",
        "rxjs": "5.0.0-beta.7",
        "zone.js": "^0.6.12",
        "moment": "2.14.1",
        "ng2-translate": "2.1.0",
        "@ngrx/core": "1.0.1",
        "@ngrx/store": "2.0.1",
