{
    "name": "ng-projects",
    "version": "1.0.0",
    "author": "LOF/MK",
    "license": "LOF",
    "description": "LOF",
    "scripts": {
        "postinstall": "npm run typings install",
        "typings": "typings",
        "start": "node server.js",
        "clean": "rm -rf dist",
        "build": "npm run clean && webpack",
        "build:w": "npm run clean && webpack -w",
        "build_prod": "npm run build && browserify -s main dist/main.js > dist/bundle.js && npm run minify",
        "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
        "build": "set NODE_ENV=production & webpack -p --optimize-minimize --optimize-dedupe"
    },
    "dependencies": {
        "@angular/common": "2.0.0-rc.4",
        "@angular/compiler": "2.0.0-rc.4",
        "@angular/core": "2.0.0-rc.4",
        "@angular/http": "2.0.0-rc.4",
        "@angular/router": "3.0.0-alpha.7",
        "@angular/platform-browser": "2.0.0-rc.4",
        "@angular/platform-browser-dynamic": "2.0.0-rc.4",
        "es6-shim": "^0.35.0",
        "reflect-metadata": "0.1.3",
        "rxjs": "5.0.0-beta.7",
        "zone.js": "^0.6.12",
        "moment": "2.14.1",
        "ng2-translate": "2.1.0",
        "@ngrx/store": "2.0.1",
        "marked": "0.3.5",
        "to-markdown": "3.0.1",
        "pikaday": "1.4.0"
    },
    "devDependencies": {
        "ts-loader": "^0.8.2",
        "typescript": "^1.8.10",
        "typings": "^0.8.1",
        "raw-loader": "^0.5.1",
        "html-webpack-plugin": "^2.16.1",
        "compression-webpack-plugin": "0.3.1",
        "webpack": "^1.13.0",
        "webpack-dev-server": "^1.14.1",
        "ngrx-store-logger": "~0.1.6"
    }
}
